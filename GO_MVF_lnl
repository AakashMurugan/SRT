#Goel-Okumoto -> Mean Value Function
GO_MVF <- function(aMLE,bMLE,data){
		tmp <- aMLE*(1-exp(-bMLE*data))
		return(tmp)
}

#Goel-Okumoto -> Failure Times -> log-likelihood

GO_FT_lnl <- function(aMLE,bMLE,data){
 		lnl <- -aMLE*(1-exp(-bMLE*tn))+n*log(aMLE)+n*log(bMLE)-bMLE*sum(data)
 		return(lnl)
}
